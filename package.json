{
  "name": "react-animated-components",
  "private": false,
  "version": "1.0.1",
  "description": "Easy CSS-in-JS-based animations for React",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": "https://github.com/jtschoonhoven/react-animated-components",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run lint && rollup --config ./rollup.config.js",
    "start": "rollup --config ./rollup.config.js --watch",
    "prettier": "npx prettier ./src --write",
    "eslint": "npx eslint ./src --fix",
    "lint": "npm run prettier && npm run eslint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "npx build-storybook --output-dir ./docs"
  },
  "author": "jtschoonhoven@gmail.com",
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-runtime": "^6.26.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.46.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "styled-components": "^5.3.0"
  }
}
